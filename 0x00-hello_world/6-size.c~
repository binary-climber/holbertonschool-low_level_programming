#include <stdio.h>
/*
 * size of variables
 */int main(void)
{
	printf("Size of a char: %lu bytes(s)\n", sizeof("a"));
	printf("Size of a char: %lu bytes(s)\n", sizeof(5));
	printf("Size of a char: %lu bytes(s)\n", sizeof(5));
	printf("Size of a char: %lu bytes(s)\n", sizeof(5));
	printf("Size of a char: %lu bytes(s)\n", sizeof(5.0));
	return (0);
}
